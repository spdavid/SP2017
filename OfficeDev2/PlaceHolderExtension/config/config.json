{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "top-menu-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/topMenu/TopMenuApplicationCustomizer.js",
          "manifest": "./src/extensions/topMenu/TopMenuApplicationCustomizer.manifest.json"
        }
      ]
    },
    "item-info-alert-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/itemInfoAlert/ItemInfoAlertCommandSet.js",
          "manifest": "./src/extensions/itemInfoAlert/ItemInfoAlertCommandSet.manifest.json"
        }
      ]
    },
    "status-indicator-field-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/statusIndicator/StatusIndicatorFieldCustomizer.js",
          "manifest": "./src/extensions/statusIndicator/StatusIndicatorFieldCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "TopMenuApplicationCustomizerStrings": "lib/extensions/topMenu/loc/{locale}.js",
    "ItemInfoAlertCommandSetStrings": "lib/extensions/itemInfoAlert/loc/{locale}.js",
    "StatusIndicatorFieldCustomizerStrings": "lib/extensions/statusIndicator/loc/{locale}.js"
  }
}
